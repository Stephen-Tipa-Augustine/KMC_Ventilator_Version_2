
<CustomCard@MDBoxLayout+ButtonBehavior>:
    size: "130dp", "64dp"

<CardArray>:
    size_hint_x: .26
    spacing: 1
    padding: 1
    orientation: 'vertical'
    MDBoxLayout:
        spacing: 1
        padding: 1
        orientation: 'horizontal'
        MDBoxLayout:
            orientation: 'vertical'
            padding: 2
            spacing: 3
            CustomCard:
                orientation: 'vertical'
                padding: "4dp"
                size_hint: None, None
                size: "130dp", "64dp"
                on_touch_down: root.open_popup(title='Change Ppeek', app=app, label_text=Ppeek.text)
                focus_behavior: True
                ripple_behavior: True
                canvas:
                    Color:
                        rgba: 0, .1, 1, 1
                    Rectangle:
                        size: self.size
                        pos: self.pos
                        source: './assets/images/card_bg1.png'
                MDLabel:
                    text: 'Ppeek'
                    halign: 'center'
                    font_size: 16
                    theme_text_color: "Custom"
                    text_color: 0, 0, 1, 1
                MDSeparator:
                    height: "1dp"
                LabelButton:
                    id: Ppeek
                    text: "46"
                    halign: 'center'
                    theme_text_color: "Custom"
                    text_color: 0,.1,1,1
                    on_release: print('Label acting as a button')
                MDSeparator:
                    height: "1dp"
                MDLabel:
                    text: "cmH20"
                    halign: 'center'
                    size_hint_y: .1
                    theme_text_color: "Custom"
                    text_color: .1,1,0,1
